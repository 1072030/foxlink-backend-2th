  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Server",
      "type": "python",
      "request": "launch",
      "module": "app.server_uvicorn",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Test",
      "type": "python",
      "request": "launch",
      "module": "test",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Background Daemon",
      "type": "python",
      "request": "launch",
      "module": "app.foxlink.daemon",
      "args": [],
      "justMyCode": false
    },
    {
      "name": "Python: FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.main:app",
        // "--reload",
        "--port",
        "80",
        "--host",
        "0.0.0.0",
        "--workers",
        "8"
      ],
      "jinja": true
    }
  ]
}